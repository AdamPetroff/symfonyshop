<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}{{ page_title|default('eshop') }}{% endblock %}</title>
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    <script src="{{ asset('assets/js/jquery-3.1.1.min.js') }}"></script>
</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <ul class="nav navbar-nav">
                <li><a href="{{ path('front_homepage') }}">Homepage</a></li>
                <li><a href="{{ path('front_blog_index') }}">Blog</a></li>
                {% if is_granted('ROLE_USER') %}
                    <li><a href="{{ logout_path() }}">Logout</a></li>
                {% else %}
                    <li><a href="{{ path('front_login') }}">Login</a></li>
                    <li><a href="{{ path('front_register') }}">Register</a></li>
                {% endif %}
            </ul>
        </div>
    </nav>
    <div id="js-flash-messages-wrapper">
        {% include 'front/_includes/_flash_messages.html.twig' %}
    </div>
    {% block containerFluid %}{% endblock %}
    {% block container %}{% endblock %}
    <footer class="text-center">
        This is a footer
    </footer>

    <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
</body>
</html>